<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sarveshtandon.www.community">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".addQuestionActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".communityQuestions"
            android:label="@string/title_activity_community_questions"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CommunityPage"
            android:label="@string/title_activity_community_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".EnterCommunity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".CreateCommunityActivity"
            android:theme="@style/AppTheme.NoActionBar"/>
        <activity android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>